#include "DIO_interface.h"
#include "SSD_interface.h"

#include <util/delay.h>

u8 rows[8]={64,38,153,121,121,153,38,64};
int main(void){

	DIO_voidInit();
	while(1)
	{
		DIO_u8SetPinValue(DIO_u8_PIN24,DIO_u8_PIN_LOW);
		DIO_u8SetPinValue(DIO_u8_PIN0,DIO_u8_PIN_HIGH);
		DIO_u8SetPinValue(DIO_u8_PIN1,DIO_u8_PIN_HIGH);
		DIO_u8SetPinValue(DIO_u8_PIN2,DIO_u8_PIN_HIGH);
		DIO_u8SetPinValue(DIO_u8_PIN3,DIO_u8_PIN_HIGH);
		DIO_u8SetPinValue(DIO_u8_PIN4,DIO_u8_PIN_HIGH);
		DIO_u8SetPinValue(DIO_u8_PIN5,DIO_u8_PIN_HIGH);
		DIO_u8SetPinValue(DIO_u8_PIN6,DIO_u8_PIN_HIGH);
		DIO_u8SetPinValue(DIO_u8_PIN7,DIO_u8_PIN_HIGH);
		DIO_u8SetPinValue(DIO_u8_PIN8,DIO_u8_PIN_HIGH);
		DIO_u8SetPinValue(DIO_u8_PIN9,DIO_u8_PIN_HIGH);
		DIO_u8SetPinValue(DIO_u8_PIN10,DIO_u8_PIN_HIGH);
		DIO_u8SetPinValue(DIO_u8_PIN11,DIO_u8_PIN_HIGH);
		DIO_u8SetPinValue(DIO_u8_PIN12,DIO_u8_PIN_HIGH);
		DIO_u8SetPinValue(DIO_u8_PIN13,DIO_u8_PIN_HIGH);

	}
	return 0 ;
}

