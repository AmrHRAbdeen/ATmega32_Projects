#include "STD_TYPES.h"
//#include "delay_ms_interface.h"
#include "DIO_interface.h"
#include "SSD_interface.h"

#include <util/delay.h>

int main(void){


	DIO_u8SetPortDirection(0,DIO_u8_PORT_OUTPUT);
	DIO_u8SetPortDirection(3,DIO_u8_PORT_OUTPUT);

	while(1)
	{
		DIO_u8SetPortnValue(0,0xFF);
		DIO_u8SetPortnValue(3,0xFF);
	}
	return 0 ;
}

