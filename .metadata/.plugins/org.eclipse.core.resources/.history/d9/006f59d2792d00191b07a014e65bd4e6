#include "STD_TYPES.h"
//#include "delay_ms_interface.h"
#include "DIO_interface.h"
#include "SSD_interface.h"

#include <util/delay.h>

u8 rows[8]={0b11111110,0b11111101,0b11111011,0b11110111,0b11101111,0b11011111,0b10111111,0b01111111};
u8 columns[8]={64,38,153,121,121,153,38,64};
int main(void){

	DIO_voidInit();
	while(1)
	{
		for(u8 i=0;i<8;i++)
		{
			DIO_u8SetPortnValue(0,rows[i]);
			_delay_ms(200);
		}


	}
	return 0 ;
}

