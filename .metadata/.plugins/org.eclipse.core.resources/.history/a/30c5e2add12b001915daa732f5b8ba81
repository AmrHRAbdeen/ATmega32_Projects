#ifndef SW_CONFIG_H_
#define SW_CONFIG_H_

/*Range:               */
/* SW_PULL_UP          */
/* SW_PULL_DWN         */

#define SW_TYPE SW_PULL_UP
#define SW_PIN  DIO_u8_PIN1

#define SW_u8_NUM_OF_SW  5
/*
range : LED_u8_FORWARD
		LED_u8_REVERSE
*/
u8 LED_Au8SWTypes[SW_u8_NUM_OF_SW]={
		SW_PULL_UP,
		SW_PULL_UP,
		SW_PULL_UP,
		SW_PULL_UP,
		SW_PULL_UP
};

u8 LED_Au8SWChannels[SW_u8_NUM_OF_SW]={
	DIO_u8_PIN0,
	DIO_u8_PIN1,
	DIO_u8_PIN2,
	DIO_u8_PIN3,
	DIO_u8_PIN4
};
#endif /* SW_CONFIG_H_ */
